<?xml version="1.0" encoding="utf-8"?>
<s:SkinnableContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
					  xmlns:s="library://ns.adobe.com/flex/spark" 
					  xmlns:mx="library://ns.adobe.com/flex/mx"
					  xmlns:navigation="ch.tofuse.micemeta.views.navigation.*"
					  skinClass="ch.tofuse.micemeta.skins.navigation.MainNavigationTabBarSkin" >
	<s:layout>
		<s:HorizontalLayout/>
	</s:layout>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import mx.collections.IList;
			
			[Bindable] public var dataProvider:IList;
		]]>
	</fx:Script>
	<navigation:ExtendedTabBar id="tabBar" dataProvider="{dataProvider}" height="100%"/>
</s:SkinnableContainer>
