<?xml version="1.0" encoding="utf-8"?>
<modulemenu:ModuleMenuEntryContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
									   xmlns:s="library://ns.adobe.com/flex/spark" 
									   xmlns:mx="library://ns.adobe.com/flex/mx" 
									   xmlns:menus="ch.tofuse.micemeta.views.menus.*"
									   insetLeft="10" xmlns:modulemenu="ch.tofuse.micemeta.views.menus.modulemenu.*">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			
			private var _label:String;
			
			public function get label():String
			{
				return menuLabel.text;
			}

			public function set label(value:String):void
			{
				menuLabel.text = value;
			}

		]]>
	</fx:Script>
	
	<s:Label id="menuLabel" 
			 styleName="menuLabel"/>
	
</modulemenu:ModuleMenuEntryContainer>
